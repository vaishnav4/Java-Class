package lec1;

public class Arithmetic_operators {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 9;
		int b = 7;
		int c;
		c = a + b;
		System.out.println(c);
		c = a - b;
		System.out.println(c);
		int d=c+a;
		System.out.println(d);
		int x=9;
		int y=11;
		int z=x*y;
		System.out.println(z);
		int p=89;
		int q=7;
		int m=p/q;
		System.out.println(m);
		int s=p%2;
		System.out.println(s);

	}

}
